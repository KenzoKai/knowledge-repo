{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Server Locally",
            "type": "python",
            "request": "launch",
            "program": "scripts/knowledge_repo",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--port",
                "7001"
            ]
        },
        {
            "name": "Run Server Locally with config",
            "type": "python",
            "request": "launch",
            "program": "scripts/knowledge_repo",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--port",
                "7001",
                "--config",
                ".config/config.py"
            ]
        },
        {
            "name": "Run Database KP Server Locally with config",
            "type": "python",
            "request": "launch",
            "program": "scripts/knowledge_repo",
            "console": "integratedTerminal",
            "args": [
                "--repo",
                "postgresql+psycopg2://knowledge_repo:password@localhost:5432/knowledge_repo:knowledge_repo",
                "runserver",
                "--port",
                "7001",
                "--config",
                ".config/config.py"
            ]
        },
        {
            "name": "Run S3 KP Server Locally with config",
